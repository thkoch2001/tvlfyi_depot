{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/strict-type-checked"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "project": true
  },
  "root": true,
  "rules": {
    "no-unused-vars": "warn",
    "prefer-const": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
